
<div class="yui-b">
	<table width="100%" cellpadding="0" cellspacing="0" border="0" class="display" id="report_table">
		<thead>
			<tr>
				<th>Options</th>
				<th>First</th>
				<th>Last</th>
				<th>Email</th>
			</tr>
		</thead>
		<tbody>
<% @developers.each do |developer| %> 
	  		<tr>
				<td class="nowrap_cell"><%= render :partial => 'options', :locals => {:developer => developer} %></td>
				<td><%= developer.first %></td>
				<td><%= developer.last %></td>
	  			<td><%= developer.email %></td>
	  		</tr>
<%	end %>
		</tbody>
	</table>
</div>

<script type="text/javascript">
  	$(document).ready(function() {
		$('#report_table').dataTable({"iDisplayLength": 25});
	});
</script>

