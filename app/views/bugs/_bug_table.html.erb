
	<table width="100%" cellpadding="0" cellspacing="0" border="0" class="display" id="bug_table">
		<thead>
			<tr>
				<th>Options</th>
				<th>Report</th>
				<th>Name</th>
				<th>Maintainer</th>
				<th>Status</th>
				<th>Date Modified</th>
			</tr>
		</thead>
		<tbody>
<% @bugs.each do |bug| %>  
	  		<%= render :partial => 'bug_tr', :locals => {:bug => bug} %>
<% end %>
		</tbody>
	</table>