<div class="new_title">Listing Export Statuses</div>

<div class="models_container_div">
<% @export_statuses.each do |export_status| %>  
  <div class="model_container_div">
  	<div class="model_description_div">
  		<%=export_status.rank %> <%=export_status.status %>
  	</div>
  	<div class="model_menu_div">
    	<div class="model_menu_button_div"><%= link_to 'Edit', edit_export_status_path(export_status) %></div>
    	<div class="model_menu_button_div"><%= link_to 'Destroy', export_status, :confirm => 'Are you sure?', :method => :delete %></div>
    </div>
  </div>
<% end %>
</div>

<div class="models_menu_div">
    <div class="model_menu_button_div">
		<%= link_to 'New', new_export_status_path %>
	</div>
</div>